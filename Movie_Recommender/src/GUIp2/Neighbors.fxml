<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="NeighborView" prefHeight="500.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <TabPane fx:id="NeighborViewTabs" layoutX="0.0" layoutY="0.0" prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
      <tabs>
        <Tab fx:id="MoviesInCommonTab" text="Movies in common">
          <content>
            <TableView fx:id="MoviesInCommonTable" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="267.0" text="Movie" fx:id="MoviesInCommonMovie" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="267.0" text="Your Rating" fx:id="CurrentUserMovieRating" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="266.0" text="Neighbor Rating" fx:id="NeighborMovieRatings" />
              </columns>
            </TableView>
          </content>
        </Tab>
        <Tab fx:id="CorrelationGraphTab" text="Correlation Graph">
          <content>
            <AnchorPane id="AnchorPane" fx:id="CorrelationGraphAnchorPane" prefHeight="469.0" prefWidth="800.0">
              <children>
                <LineChart fx:id="CorrelationGraphLineView" layoutX="0.0" layoutY="0.0" prefHeight="469.0" prefWidth="800.0" visible="true">
                  <xAxis>
                    <NumberAxis id="CurrentUserScores" fx:id="CurrentUserScoresLine" autoRanging="true" forceZeroInRange="true" label="Your Ratings" lowerBound="1.0" minorTickVisible="true" opacity="0.48" side="BOTTOM" tickLabelsVisible="true" tickMarkVisible="true" upperBound="5.5" visible="false" />
                  </xAxis>
                  <yAxis>
                    <NumberAxis id="NeighborScores" fx:id="NeighborScoresLine" autoRanging="true" forceZeroInRange="true" label="Neighbor Ratings" lowerBound="1.0" minorTickVisible="true" opacity="0.48" side="LEFT" tickLabelsVisible="true" tickMarkVisible="true" upperBound="5.5" visible="false" />
                  </yAxis>
                </LineChart>
                <ScatterChart fx:id="CorrelationGraphScatterChart" layoutX="0.0" layoutY="0.0" prefHeight="469.0" prefWidth="800.0">
                  <xAxis>
                    <NumberAxis id="Your" fx:id="CurrentUserScoresScatter" label="Your Ratings" lowerBound="1.0" side="BOTTOM" upperBound="5.5" />
                  </xAxis>
                  <yAxis>
                    <NumberAxis fx:id="NeighborScoresScatter" label="Neighbor Ratings" lowerBound="1.0" side="LEFT" upperBound="5.5" />
                  </yAxis>
                </ScatterChart>
              </children>
            </AnchorPane>
          </content>
        </Tab>
      </tabs>
    </TabPane>
  </children>
</AnchorPane>
